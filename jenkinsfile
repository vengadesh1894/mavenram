pipeline
{
    agent any
    stages
    {
        stage('continousDownload')
        {
            steps
            {
                git 'https://github.com/vengadesh1894/maven.git'
            }
        }
        stage('continousBuild')
        {
            steps
            {
                sh 'mvn package'
            }
        }
                
            }
        }
    }
}    

